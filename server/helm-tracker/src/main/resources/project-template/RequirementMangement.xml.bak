<?xml version="1.0" encoding="UTF-8"?>

<template name="需求管理项目模版" description="需求管理项目模版" iconUrl="" imageUrl="">
    <roles>
        <role name="项目管理员" specialRole="false">
            <members></members>
        </role>
        <role name="需求工程师" specialRole="false"/>
        <role name="质量保证工程师" specialRole="false"/>
        <role name="功能安全工程师" specialRole="false"/>
        <role name="硬件工程师" specialRole="false"/>
        <role name="软件工程师" specialRole="false"/>
        <role name="外部合作伙伴" specialRole="false"/>
    </roles>
    <permissions>
        <permission name="PROJECT_ADMIN">
            <grant-to-role>项目管理员</grant-to-role>
            <grant-to-special-role>项目负责人</grant-to-special-role>
            <!--            <grant-to-user></grant-to-user>-->
        </permission>
        <permission name="PROJECT_VIEW">
            <grant-to-special-role>所有项目成员</grant-to-special-role>
        </permission>
    </permissions>
    <trackers>
        <tracker name="发布" shortName="RELEASE" type="RELEASE" visible="true" workflow="true" defaultLayout="table">
            <statuses>
                <status name="未开始" meaning="未开始" initial="true"/>
                <status name="未发布" meaning="进行中"/>
                <status name="已发布" meaning="已解决"/>
                <status name="生命周期结束" meaning="已关闭"/>
            </statuses>
            <permissions>
                <permission name="ITEM_VIEW">
                    <grant-to-special-role>所有项目成员</grant-to-special-role>
                </permission>
                <permission name="ITEM_CREATE">
                    <grant-to-special-role>所有项目成员</grant-to-special-role>
                </permission>
                <permission name="ITEM_EDIT">
