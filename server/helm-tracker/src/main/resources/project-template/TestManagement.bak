        <role name="Project Admin" displayAsRole="true" autoIncludeProjectOwner="true"
              permissions="PROJECT_ADMIN，PROJECT_VIEW"/>
        <role name="Test Engineer" displayAsRole="true"
              permissions="PROJECT_VIEW"/>
        <role name="Test Lead" displayAsRole="true"
              permissions="PROJECT_VIEW"/>
        <role name="Tester" displayAsRole="true"
              permissions="PROJECT_VIEW"/>
    </roles>
